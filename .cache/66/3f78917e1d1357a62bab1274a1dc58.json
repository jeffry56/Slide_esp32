source":"css/style1.css","original":{"line":233,"column":0},"generated":{"line":233,"column":0}},{"source":"css/style1.css","original":{"line":234,"column":0},"generated":{"line":234,"column":0}},{"source":"css/style1.css","original":{"line":235,"column":0},"generated":{"line":235,"column":0}},{"source":"css/style1.css","original":{"line":236,"column":0},"generated":{"line":236,"column":0}},{"source":"css/style1.css","original":{"line":237,"column":0},"generated":{"line":237,"column":0}}],"sources":{"css/style1.css":"body {\n  /*background:#e4e5f5 none repeat scroll 0 0;*/\n  background-image: URL(\"bg_01.jpg\");\n  /*background-repeat:repeat-x;*/\n  color: #222;\n  font-family: \"Raleway\", sans-serif;\n  font-size: 100%;\n  line-height: 24px;\n  margin: 0;\n  padding: 0;\n}\n\n.page-title {\n  color: rgb(70, 101, 201);\n  text-align: center;\n}\n.page-container {\n  padding: 10px;\n}\n\n.external-grid {\n  display: grid;\n  grid-template-columns: 48% 48%;\n  grid-gap: 4%;\n}\n\n.panel-header {\n  font-size: 18px;\n  font-weight: bold;\n  text-align: center;\n  margin-top: 20px;\n  margin-bottom: 10px;\n  padding: 10px;\n  border-style: ridge;\n}\n\n.panel-footer {\n  text-align: center;\n  margin-top: 10px;\n  padding: 10px;\n  display: flex;\n  border-style: ridge;\n}\n.panel-footer .text {\n  font-size: 20px;\n  font-weight: bold;\n  flex: 1;\n}\n.panel-footer .value {\n  font-size: 20px;\n  font-weight: bold;\n  padding: 0 40px;\n}\n\n.grid-button {\n  height: 80px;\n  width: 80px;\n  border-radius: 100%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  border: none;\n  cursor: pointer;\n}\n\n.grid-button:hover {\n}\n\n.panel-grid {\n  display: grid;\n  grid-template-columns: 32% 32% 32%;\n  grid-template-rows: 150px 150px;\n  grid-gap: 2%;\n}\n\n.grid-item {\n  /*background-color: red;*/\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  flex-direction: column;\n  padding: 10px;\n}\n\n.grid-item-title {\n  background-color: blueviolet;\n  width: 100%;\n  text-align: center;\n  margin-bottom: 10px;\n}\n\n.grid-item-content {\n}\n\n/*Button Design*/\n.buttons {\n  margin-top: 160px;\n  margin: 0 auto;\n  text-align: center;\n}\n\n.buttons a {\n  margin-left: 10px;\n  margin-right: 10px;\n  margin-top: 6px;\n  margin-bottom: 10px;\n  display: inline-block;\n  position: relative;\n  line-height: 60px;\n  background-color: #eaeaea;\n  background-image: -webkit-gradient(\n    linear,\n    left top,\n    left bottom,\n    from(#f6f6f6),\n    to(#eaeaea)\n  );\n  background-image: -webkit-linear-gradient(top, #f6f6f6, #eaeaea);\n  background-image: -moz-linear-gradient(top, #f6f6f6, #eaeaea);\n  background-image: -ms-linear-gradient(top, #f6f6f6, #eaeaea);\n  background-image: -o-linear-gradient(top, #f6f6f6, #eaeaea);\n  background-image: linear-gradient(top, #f6f6f6, #eaeaea);\n  -moz-border-radius: 32px;\n  -webkit-border-radius: 32px;\n  border-radius: 32px;\n  -moz-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.25), 0 2px 3px rgba(0, 0, 0, 0.1);\n  -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.25),\n    0 2px 3px rgba(0, 0, 0, 0.1);\n  box-shadow: 0 1px 1px rgba(0, 0, 0, 0.25), 0 2px 3px rgba(0, 0, 0, 0.1);\n}\n\n.buttons a:active {\n  top: 5px;\n  background-image: -webkit-gradient(\n    linear,\n    left top,\n    left bottom,\n    from(#eaeaea),\n    to(#f6f6f6)\n  );\n  background-image: -webkit-linear-gradient(top, #eaeaea, #f6f6f6);\n  background-image: -moz-linear-gradient(top, #eaeaea, #f6f6f6);\n  background-image: -ms-linear-gradient(top, #eaeaea, #f6f6f6);\n  background-image: -o-linear-gradient(top, #eaeaea, #f6f6f6);\n  background-image: linear-gradient(top, #eaeaea, #f6f6f6);\n}\n\n.buttons a::before {\n  content: \"\";\n  position: absolute;\n  z-index: -1;\n  top: -8px;\n  right: -8px;\n  bottom: -8px;\n  left: -8px;\n  background-color: #eaeaea;\n  -moz-border-radius: 140px;\n  -webkit-border-radius: 140px;\n  border-radius: 140px;\n  opacity: 0.5;\n}\n\n.buttons a:active::before {\n  top: -9px;\n}\n\n.buttons a:hover::before {\n  opacity: 1;\n}\n\n.buttons.large input {\n  top: 0;\n  right: 0;\n  bottom: 0;\n  left: 0;\n  opacity: 1;\n  z-index: 100;\n  position: absolute;\n  width: 100%;\n  height: 100%;\n  cursor: pointer;\n  background: transparent;\n}\n\n.buttons a.maroon input:checked ~ label {\n  color: #5cd604;\n  top: 50;\n}\n\n/*Buttons sizes*/\n.large a {\n  width: 64px;\n  height: 64px;\n}\n\n.medium a {\n  width: 54px;\n  height: 54px;\n}\n\n.small a {\n  width: 44px;\n  height: 44px;\n}\n\n/*Hover Effect*/\n.buttons a.blue:hover::before {\n  background-color: #c6f0f8;\n}\n.buttons a.orange:hover::before {\n  background-color: #fae2c6;\n}\n.buttons a.pink:hover::before {\n  background-color: #f6d2e4;\n}\n.buttons a.red:hover::before {\n  background-color: #ffc3c3;\n}\n.buttons a.purple:hover::before {\n  background-color: #ecbbef;\n}\n.buttons a.mustard:hover::before {\n  background-color: #faefc7;\n}\n.buttons a.seaweed:hover::before {\n  background-color: #cfeef2;\n}\n.buttons a.maroon:hover::before {\n  background-color: #f3c2d5;\n}\n.buttons a.liver:hover::before {\n  background-color: #c3aca7;\n}\n.buttons a.black:hover::before {\n  background-color: #d2cecd;\n}\n"},"lineCount":237}},"error":null,"hash":"e5bbd1280a227e6920fa1db5158ab2f7","cacheData":{"env":{}}}